import random

func main() {
    number = random.randint(0, 1000)

    while (true) {
        guess = int(input("Guess The Number from 0-1000: "))

        if (guess < number) {
            print("To low!")
        }

        else if (guess > number) {
            print("To large!")
        }

        else {
            print("You correct!")
            break
        }
    }
}

if (__name__ == '__main__') {
    main()
}