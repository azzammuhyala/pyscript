Hewan = type('Hewan', (), {
    '__init__': func(self, jenis, nama) {
        self.jenis = jenis
        self.nama = nama
    },
    'bersuara': func(self) {
        print("Sebuah {} bernama {} bersuara...".format(self.jenis.lower(), self.nama))
    }
})

Kucing = type('Kucing', (Hewan,), {
    '__init__': func(self, nama) {
        super(Kucing, self).__init__("Kucing", nama)
    }
})

kucing = Kucing("Awan")

kucing.bersuara()