class jsdict extends dict {

    func __getattribute__(self, name) {
        return name in self ? self[name] : None
    }

    func __setattr__(self, name, value) {
        self[name] = value
    }

    func __delattr__(self, name) {
        if (name in self) {
            del self[name]
        }
    }

}

__all__ = (
    'jsdict'
)